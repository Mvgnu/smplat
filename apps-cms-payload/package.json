{
  "name": "@smplat/cms-payload",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "PAYLOAD_CONFIG_PATH=./src/payload.config.ts next dev -p 3050",
    "build": "PAYLOAD_CONFIG_PATH=./src/payload.config.ts next build",
    "start": "PAYLOAD_CONFIG_PATH=./src/payload.config.ts next start -p 3050",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "generate:importmap": "payload generate:importmap --config ./src/payload.config.ts --output ./importMap.js",
    "generate:types": "payload generate:types --config ./src/payload.config.ts --output ./payload-types.ts",
    "test:int": "vitest run"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.61.1",
    "@payloadcms/next": "^3.61.1",
    "@payloadcms/richtext-lexical": "^3.61.1",
    "dotenv": "^16.4.5",
    "next": "15.2.3",
    "payload": "^3.61.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.6",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
